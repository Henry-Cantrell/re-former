<%= form_with model: @user, method: :put do |form| %>
    <br><br>
    <%= form.label :username, "Username:" %>
    <%= form.text_field :username, :value => 'Your username here'%><br><br>
    <%= form.label :email, "Email:" %>
    <%= form.text_field :email, :value => 'user@domain.com' %><br><br>
    <%= form.label :password, "Password:" %>
    <%= form.password_field :password %><br><br>
    <%= form.submit "Submit" %>
  <% end %>

  <p></p> 
  
  <%= link_to "Home", root_path %>